{
    "name": "HakuNeko Assistant",
    "version": "2024.05.21",
    "manifest_version": 3,
    "icons": {
        "16": "assets/icon-16.png",
        "32": "assets/icon-32.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "cookies",
        "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}